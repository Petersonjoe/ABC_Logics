              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset">[dbo].[tmp_ccs]</property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">declare @sql1 nvarchar(max)
declare @sql2 nvarchar(max)
declare @sql nvarchar(max)
declare @month nvarchar(6)
declare @i int = 0
declare @count int

if object_id(N'tempdb..#tmp_list') is not null
  drop table #tmp_list

if object_id(N'dbo.tmp_ccs') is not null
  drop table dbo.tmp_ccs

select table_name
into #tmp_list
from information_schema.tables
where table_type = 'BASE TABLE'
and table_name like 'CCS_20[0-9][0-9][0-9][0-9]'
order by table_name

select @count = count(*) from #tmp_list


while @count &gt; 0
begin
  set @i = @i + 1
  if @i = 1
  begin
    select @sql1 = (select top 1 table_name from #tmp_list)
	select @sql2 = right(@sql1,6) 

    set @sql = '
     select 
	        ''' + @sql2 + ''' as [Month], 
			[Part No], 
			[Vendor No], 
			[Contract Cost], 
			[Forecast Cost],
			[Region], 
			[Transaction Type] 
	   into dbo.tmp_ccs
	   from ' + @sql1;

	--print @sql
	exec sp_sqlexec @sql
	delete from #tmp_list where table_name = @sql1
  end
  else
  begin
    select @sql1 = (select top 1 table_name from #tmp_list)
	select @sql2 = right(@sql1,6) 

    set @sql = '
     insert into dbo.tmp_ccs
	 select
	        ''' + @sql2 + ''' as [Month], 
			[Part No], 
			[Vendor No], 
			[Contract Cost], 
			[Forecast Cost],
			[Region], 
			[Transaction Type] 
	   from ' + @sql1;

	--print @sql
	exec sp_sqlexec @sql
	delete from #tmp_list where table_name = @sql1
  end
  set @count = @count - 1
end

select * from dbo.tmp_ccs</property>
