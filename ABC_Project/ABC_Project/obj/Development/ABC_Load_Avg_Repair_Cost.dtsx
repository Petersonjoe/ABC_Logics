      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;WITH T AS( SELECT A.[Region] AS [Region A]&#xA;      ,A.[Material] AS [Material A]&#xA;&#x9;  ,A.[OOW Avg Repair Cost]&#xA;      ,B.[Region] AS [Region B]&#xA;&#x9;  ,B.Material AS [Material B]&#xA;&#x9;  ,B.[OOW/IW Avg Repair Cost]&#xA;  FROM (&#xA;&#x9;&#x9;SELECT [Region]&#xA;&#x9;&#x9;&#x9;  ,[Material]&#xA;&#x9;&#x9;&#x9;  ,sum(abs([PO&amp;DF Spend]))/sum(abs([PO&amp;DF Qty])) as [OOW Avg Repair Cost]&#xA;&#x9;&#x9;  FROM [E2.2].[dbo].[WW_Order_CIM]&#xA;&#x9;&#x9; WHERE [Sub_Order_Type] in ('YI1P','ZSR','ZSRA')&#xA;&#x9;&#x9;   AND [Fiscal_Year] = '&quot; + @[User::FiscalY] +&#xA;&#x9;&#x9; &quot;' AND [Fiscal_Qtr]  = '&quot; + @[User::FiscalQ] + &#xA;&#x9;   &quot;' GROUP BY [Region]&#xA;&#x9;&#x9;&#x9;  ,[Material]) A&#xA;FULL OUTER JOIN (&#xA;&#x9;&#x9;&#x9;SELECT [Region]&#xA;&#x9;&#x9;&#x9;&#x9;  ,[Material]&#xA;&#x9;&#x9;&#x9;&#x9;  ,sum(abs([PO&amp;DF Spend]))/sum(abs([PO&amp;DF Qty])) as [OOW/IW Avg Repair Cost]&#xA;&#x9;&#x9;&#x9;  FROM [E2.2].[dbo].[WW_Order_CIM]&#xA;&#x9;&#x9;&#x9; WHERE [Sub_Order_Type] in ('YI1P','ZSR','ZSRA','ZSX')&#xA;&#x9;&#x9;&#x9;   AND [Fiscal_Year] = '&quot; + @[User::FiscalY] +&#xA;&#x9;&#x9;&#x9; &quot;' AND [Fiscal_Qtr]  = '&quot; + @[User::FiscalQ] +&#xA;&#x9;&#x9;   &quot;' GROUP BY [Region]&#xA;&#x9;&#x9;&#x9;&#x9;  ,[Material]) B&#xA;ON A.Region = B.Region AND A.Material = B.Material)&#xA;SELECT &#xA;     Region = CASE WHEN ISNULL([Region A],'') = '' AND [Region B] = 'AMS' THEN 'AME'&#xA;&#x9;               WHEN ISNULL([Region A],'') = '' AND [Region B] != 'AMS' THEN [Region B]&#xA;                   WHEN ISNULL([Region B],'') = '' AND [Region A] = 'AMS' THEN 'AME'&#xA;&#x9;&#x9;&#x9;&#x9;   WHEN ISNULL([Region B],'') = '' AND [Region A] != 'AMS' THEN [Region A]&#xA;&#x9;&#x9;&#x9;       WHEN ISNULL([Region B],'') != '' AND ISNULL([Region A],'') != ''&#xA;&#x9;&#x9;&#x9;&#x9;    AND [Region B] = 'AMS' THEN 'AME'&#xA;&#x9;&#x9;&#x9;&#x9;   ELSE [Region B]&#xA;&#x9;&#x9;&#x9;  END,&#xA;&#x9; Material = CASE WHEN ISNULL([Material A],'') = '' THEN [Material B]&#xA;                     WHEN ISNULL([Material B],'') = '' THEN [Material A]&#xA;&#x9;&#x9;&#x9;&#x9;&#x9; ELSE [Material B]&#xA;&#x9;&#x9;&#x9;&#x9;END,&#xA;     [OOW Avg Repair Cost],&#xA;&#x9; [OOW/IW Avg Repair Cost]&#xA;FROM T&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="CalcLogic">
      <DTS:VariableValue
        DTS:DataType="8">WITH T AS( SELECT A.[Region] AS [Region A]
      ,A.[Material] AS [Material A]
	  ,A.[OOW Avg Repair Cost]
      ,B.[Region] AS [Region B]
	  ,B.Material AS [Material B]
	  ,B.[OOW/IW Avg Repair Cost]
  FROM (
		SELECT [Region]
			  ,[Material]
			  ,sum(abs([PO&amp;DF Spend]))/sum(abs([PO&amp;DF Qty])) as [OOW Avg Repair Cost]
		  FROM [E2.2].[dbo].[WW_Order_CIM]
		 WHERE [Sub_Order_Type] in ('YI1P','ZSR','ZSRA')
		   AND [Fiscal_Year] = '2017' AND [Fiscal_Qtr]  = 'Q3' GROUP BY [Region]
			  ,[Material]) A
FULL OUTER JOIN (
			SELECT [Region]
				  ,[Material]
				  ,sum(abs([PO&amp;DF Spend]))/sum(abs([PO&amp;DF Qty])) as [OOW/IW Avg Repair Cost]
			  FROM [E2.2].[dbo].[WW_Order_CIM]
			 WHERE [Sub_Order_Type] in ('YI1P','ZSR','ZSRA','ZSX')
			   AND [Fiscal_Year] = '2017' AND [Fiscal_Qtr]  = 'Q3' GROUP BY [Region]
				  ,[Material]) B
ON A.Region = B.Region AND A.Material = B.Material)
SELECT 
     Region = CASE WHEN ISNULL([Region A],'') = '' AND [Region B] = 'AMS' THEN 'AME'
	               WHEN ISNULL([Region A],'') = '' AND [Region B] != 'AMS' THEN [Region B]
                   WHEN ISNULL([Region B],'') = '' AND [Region A] = 'AMS' THEN 'AME'
				   WHEN ISNULL([Region B],'') = '' AND [Region A] != 'AMS' THEN [Region A]
			       WHEN ISNULL([Region B],'') != '' AND ISNULL([Region A],'') != ''
				    AND [Region B] = 'AMS' THEN 'AME'
				   ELSE [Region B]
			  END,
	 Material = CASE WHEN ISNULL([Material A],'') = '' THEN [Material B]
                     WHEN ISNULL([Material B],'') = '' THEN [Material A]
					 ELSE [Material B]
				END,
     [OOW Avg Repair Cost],
	 [OOW/IW Avg Repair Cost]
FROM T</DTS:VariableValue>
    </DTS:Variable>
